#include <iostream>
using namespace std;

int main() {
    char name [30];
    for (int i = 0; i < 30; i++){
        name[i] = getchar ();
        if (name[i] == ' ') {
            break;
        }
    }
    
    for (int i = 0; i < 30; i++) {
        if (name[i] == ' ') {
            break;
        }
        cout << name[i];
    }
    return 0;
}
	
